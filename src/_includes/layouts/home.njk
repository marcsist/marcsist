---
layout: layouts/base.njk
templateClass: tmpl-home
---

<div class="wrapper home">
  <div class="content">
    {{ content | safe }}
    

      
    <section id="notes">
      <h2><b>Notes</b></h2>
      <ul class="no-bullet" role="list">
        {%- for post in collections.posts | reverse -%}
          <li{% if page.url == post.url %} aria-current="page"{% endif %} role="listitem">
            <time datetime="{{ post.data.page.date | htmlDateString }}">{{ post.data.page.date | htmlDateString }}</time> â€” 
            <a href='{{ post.url }}'>{{ post.data.title }}</a>
          </li>
        {%- endfor -%}
        <li role="listitem"><a href="/notes">View all notes</a></li>
      </ul>
    </section>
    {% include 'layouts/blocks/things.njk' %}
    

  </div>
</div>
